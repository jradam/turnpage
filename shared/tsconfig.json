{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [ "ES2022" ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [ "**/*.ts" ]
}
